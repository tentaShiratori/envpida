{
  "name": "envpida",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "https://github.com/tentaShiratori/envpida.git",
  "author": "tentaShiratori <tenta.shiratori@gmail.com>",
  "license": "MIT",
  "types": "dist/cli.d.ts",
  "bin": "bin/index.js",
  "scripts": {
    "dev": "npm run build && node ./bin/index.js",
    "build": "npm run rimraf && tsc",
    "rimraf": "node -e \"require('fs').existsSync('dist') && require('fs').rmSync('dist', { recursive: true, force: true })\"",
    "lint": "eslint",
    "fmt": "prettier --write",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "commander": "^10.0.1",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.1.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "pinst": "^3.0.0",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "4.9.3"
  }
}
